CREATE TABLE USER (NICKNAME VARCHAR(255) NOT NULL, DESCRIPTION VARCHAR(255), EMAIL VARCHAR(255), FULLNAME VARCHAR(255), HOST TINYINT(1) default 0, PASSWORD VARCHAR(255), RATING DOUBLE, TRAVELER TINYINT(1) default 0, VERSION INTEGER, PRIMARY KEY (NICKNAME))
CREATE TABLE HOST (ID INTEGER AUTO_INCREMENT NOT NULL, LISTOFINTERESTS LONGBLOB, VERSION INTEGER, USER_NICKNAME VARCHAR(255), PRIMARY KEY (ID))
ALTER TABLE HOST ADD CONSTRAINT FK_HOST_USER_NICKNAME FOREIGN KEY (USER_NICKNAME) REFERENCES USER (NICKNAME)
